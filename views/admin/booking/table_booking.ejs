<!-- DataTales Example -->
<div class="card shadow mb-4">
  <div class="card-body">
    <div class="table-responsive">
      <table
        class="table table-bordered"
        id="dataTable"
        width="100%"
        cellspacing="0"
      >
        <thead>
          <tr>
            <th>No.</th>
            <th>Invoice</th>
            <th>Date</th>
            <th>Title</th>
            <th>Name Member</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% for(i = 0; i < booking.length; i++) { %>
          <tr>
            <td><%= i + 1 %></td>
            <td>#04<%= booking[i].invoice %></td>
            <td>
              <%= booking[i].bookingStartDate.toLocaleString().split(',')[0] %>
              s/d <%= booking[i].bookingEndDate.toLocaleString().split(',')[0]
              %>
            </td>
            <td><%= booking[i].itemId.title %></td>
            <td>
              <%= booking[i].memberId.firstName %> <%=
              booking[i].memberId.lastName %>
            </td>
            <td><%= booking[i].payments.status %></td>
            <td>
              <a
                class="btn btn-success btn-circle btn-sm button-update"
                href="/admin/booking/<%= booking[i].id %>"
                ><i class="fas fa-eye"></i
              ></a>
            </td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </div>
</div>
