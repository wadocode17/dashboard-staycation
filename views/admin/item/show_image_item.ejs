<div
  class="tab-pane fade show active"
  id="show-image"
  role="tabpanel"
  aria-labelledby="home-tab"
>
  <!-- DataTales Example -->
  <div class="row">
    <% for(let x = 0; x < item.imageId.length; x++) { %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-1">
        <div class="card-body">
          <img
            src="https://dashboardstaycation.herokuapp.com/<%- item.imageId[x].imageUrl %>"
            alt="Image"
            style="width: 100%; display: block;"
            ;
          />
        </div>
      </div>
    </div>
    <% } %>
  </div>
  <a href="/admin/item" class="btn btn-warning">Back</a>
</div>
